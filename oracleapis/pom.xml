            <dependency>
                <groupId>com.fasterxml.jackson.core</groupId>
                <artifactId>jackson-core</artifactId>
                <version>${jackson.core.version}</version>
            </dependency>
            <dependency>
                <groupId>com.fasterxml.jackson.core</groupId>
                <artifactId>jackson-databind</artifactId>
                <version>${jackson.core.version}</version>
            </dependency>
            <!-- Common Frameworks - End -->
            
            
        </dependencies>
    </dependencyManagement>
    
  <modules>
    <module>apichallenge-rest</module>
    <module>apichallenge-service</module>
    <module>apichallenge-model</module>
    <module>apichallenge-dao</module>
  </modules>
</project>